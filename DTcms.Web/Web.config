<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <!-- appSettings网站信息配置-->
  <appSettings>
    <add key="Configpath" value="~/xmlconfig/site.config" />
    <add key="Urlspath" value="~/xmlconfig/urls.config" />
    <add key="Userpath" value="~/xmlconfig/user.config" />
    <add key="Orderpath" value="~/xmlconfig/order.config" />
  </appSettings>
    <!-- 数据库连接字符串-->
    <connectionStrings>
      <!--<add name="ConnectionString" connectionString="server=10.0.0.4;uid=sa;pwd=sa123456;database=ql;" />-->
      <!--<add name="ConnectionString" connectionString="server=118.244.213.179,1533;uid=sa;pwd=sa@@888;database=ql;" />-->
      <!--<add name="ConnectionString" connectionString="server=10.0.0.4,1533;uid=sa;pwd=sa123456;database=ql;" />-->
      <!--<add name="ConnectionString" connectionString="server=172.16.110.1,1533;uid=sa;pwd=sa123456;database=ql;" />-->
      <add name="ConnectionString" connectionString="server=.;uid=sa;pwd=123456;database=ql;" />
      <!--<add name="ConnectionString" connectionString="server=.;uid=sa;pwd=123456;database=ql;" />-->
    </connectionStrings>
    <system.web>
        <compilation debug="true">

        </compilation>
        <!--
            通过 <authentication> 节可以配置
            安全身份验证模式，ASP.NET 
            使用该模式来识别来访用户身份。 
        -->
        <authentication mode="Windows" />
        <!--
            如果在执行请求的过程中出现未处理的错误，
            则通过 <customErrors> 节
            可以配置相应的处理步骤。具体而言，
            开发人员通过该节可配置要显示的 html 错误页，
            以代替错误堆栈跟踪。

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
      <httpModules>
        
        <add type="DTcms.Web.UI.HttpModule, DTcms.Web.UI" name="HttpModule" />
      </httpModules>


      <httpHandlers>
        <add verb="*" path="templates/green/*.html" type="System.Web.HttpForbiddenHandler" />
        <add verb="*" path="templates/mobile/*.html" type="System.Web.HttpForbiddenHandler" />
      </httpHandlers>
      <!-- 文件上传大小KB-->
      <httpRuntime maxRequestLength="512000" executionTimeout="3600" />

    </system.web>

</configuration>
